  "scripts": {
    "start": "REACT_APP_BASE_URL=https://jakub.datim.org/ craco start",
    "start:hard": "rm -rf node_modules/.cache && npm start",
    "build:react": "react-scripts build",
    "build:zip": "(rm -f dashboard-information-widget.zip && cd build && zip ../dashboard-information-widget.zip -r .)",
    "build": "npm run build:react && npm run build:zip",
    "test": "react-scripts test",
    "test:cache": "rm cachedApiCalls/*",
    "dep:build": "ts-node --skip-project packages/buildWatch.ts",
    "dep:copy": "ts-node --skip-project packages/copyWatch.ts"
