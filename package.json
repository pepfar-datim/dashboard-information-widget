{
  "name": "dashboard-information",
  "version": "1.1.3",
  "private": true,
  "dependencies": {
    "@dhis2/ui": "^8.4.15",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.5",
    "@pepfar-react-lib/datim-api": "^0.0.23",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "jodit": "^3.20.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.4.0",
    "sanitize-html": "^2.7.2",
    "styled-jsx": "^5.0.7",
    "yaml": "^2.1.1",
    "@pepfar-react-lib/message-provider": "^0.0.1"
  },
  "homepage": ".",
  "scripts": {
    "start": "REACT_APP_BASE_URL=https://jakub.testing.datim.org/ craco start",
    "start:hard": "rm -rf node_modules/.cache && npm start",
    "build": "REACT_APP_BASE_URL=../../.. craco build && npm run build:zip",
    "build:zip": "(rm -f dashboard-information-widget.zip && cd build && zip ../dashboard-information-widget.zip -r .)",
    "test": "REACT_APP_BASE_URL=https://jakub.testing.datim.org/ craco test --maxWorkers=6 --verbose",
    "test:cache": "rm cachedApiCalls/*",
    "dep:copy": "node --experimental-specifier-resolution=node node_modules/@pepfar-react-lib/dep-copy/build/bin/dep-copy.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@pepfar-react-lib/dep-copy": "^0.0.21",
    "@pepfar-react-lib/testwrap": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "ts-node": "^10.9.1",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.2",
    "craco": "^0.0.3",
    "react-scripts": "5.0.1",
    "ts-loader": "^9.4.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@pepfar-react-lib|node-fetch|data-uri-to-buffer|fetch-blob|formdata-polyfill/.*)"
    ]
  }
}
