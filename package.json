  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@pepfar-react-lib/testwrap": "^0.0.13",
    "@pepfar-react-lib/dep-copy": "^0.0.21",
    "@testing-library/jest-dom": "^5.16.5"
  }
